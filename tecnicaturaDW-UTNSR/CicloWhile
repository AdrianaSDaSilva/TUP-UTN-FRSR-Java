package CicloWhile;

public class EjercicioWhile01 {
    public static void main (String[] args){
        var conteo = 0; // Inferencia de tipos
        while ( conteo < 8) {
            System.out.println("conteo =" + conteo);
            conteo++; // vamos aumentado en uno la variable
      
        }
    }
            
}

